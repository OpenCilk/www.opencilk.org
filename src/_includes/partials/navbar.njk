<nav class="bd-header navbar navbar-dark navbar-expand-lg bd-navbar" id="navbar-main"><div class="bd-header__inner bd-page-width">
    <label class="sidebar-toggle primary-toggle" for="__primary">
        <span class="fas fa-bars"></span>
    </label>
    <div id="navbar-start">
        <a class="navbar-brand logo" href="/">
            <img src="{{ sphinxDir }}_static/OpenCilk-icon-330.png" class="logo__image" alt="Logo image">
        </a>
    </div>

    <div class="col-lg-9 navbar-header-items">
        <div id="navbar-center" class="mr-auto">
        
            <div class="navbar-center-item">
                <nav class="mynavbar-nav">
                    <ul id="navbar-main-elements" class="mynavbar-nav">
                        {% for entry in collections.all | eleventyNavigation("Home") %}
                        <li class="mynav-item">
                            {% if page.url == entry.url %}
                            <a class="mynav-link active"  aria-current="page" href="{{ entry.url | url }}">{{ entry.title }}</a>
                            {% else %}
                            <a class="mynav-link" href="{{ entry.url | url }}">{{ entry.title }}</a>
                            {% endif %}
                        </li>
                        {% endfor %}
                    </ul>
                </nav>
            </div>
        
        </div>

        <div id="navbar-end">
            <div class="navbar-end-item navbar-end__search-button-container">        
                <form>
                  <div class="w-64">
                    <div id="search"></div>
                  </div>
                </form>
            </div>
        </div>
    </div>

    <label class="sidebar-toggle secondary-toggle" for="__secondary">
        <span class="fas fa-outdent"></span>
    </label>
</nav>
