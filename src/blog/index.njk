---
title: Go Multicore
images:
  placement:  # options: feature, background
  background: grey-silk-bg.jpg
  align: object-center # object-center (default) - other options at https://tailwindcss.com/docs/object-position
hero:
  title: Go Multicore
  subtitle: 
  links:
layout: pages
sidebar: Blog
eleventyNavigation:
  key: Blog
  parent: home
  order: 3
pagination:
  data: collections.post
  size: 4
  reverse: true
---

<section class="mt-15">
    {% set postslist = pagination.items %}

    {% include 'partials/postslistcards.njk' %}

    {% for tag in collections.tagList %}
      {% set tagUrl %}/tags/{{ tag | slug }}/{% endset %}
      <a href="{{ tagUrl | url }}" class="post-tag">{{ tag }}</a>
    {% endfor %}

    {% if collections.post.length > pagination.size %}
        {% include "partials/pagination.njk" %}
    {% endif %}
</section>